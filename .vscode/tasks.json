    "version": "2.0.0",
    "tasks": [
        {
            "label": "Swag init",
            "type": "shell",
            "command": "swag init --parseDependency",
            "problemMatcher": []
        },
        {
            "label": "Air",
            "type": "shell",
            "command": "air",
            "problemMatcher": []
        },
        {
            "label": "Uvicorn fast api 8000",
            "type": "shell",
            "command": "${command:python.interpreterPath}",
            "args": [
                "-m",
                "uvicorn",
                "main:app",
                "--reload"
            ],
            "problemMatcher": []
        }
    ]
}